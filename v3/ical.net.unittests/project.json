{
    "version": "1.0.0-*",

    "dependencies": {
        "ical.net": "1.0.0-*",
        "ical.net.collections": "1.0.0-*",
        "NETStandard.Library": "1.6.0",
        "NUnit": "3.4.1",
        "dotnet-test-nunit": "3.4.0-beta-2",
        "System.Linq.Parallel": "4.0.1",
        "System.Linq": "4.1.0",
        "System.Linq.Queryable": "4.0.1"
    },
    "testRunner": "nunit",

    "frameworks": {
        "netcoreapp1.0": {
            "imports": [
                "netcoreapp1.0",
                "portable-net45+win8"
            ],
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "version": "1.0.0-*",
                    "type": "platform"
                }
            }
        }
    },
    "buildOptions": {
        "embed": {
            "include": [
                "Calendars/Alarm/*.ics",
                "Calendars/Journal/*.ics",
                "Calendars/Recurrence/*.ics",
                "Calendars/Serialization/*.ics",
                "Calendars/Todo/*.ics"
            ]
        }
    }
}
